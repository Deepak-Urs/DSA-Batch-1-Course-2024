class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int[] ans=new int[nums.length];
        for(int i=0;i<nums.length;i++){
            int count=0;
            for(int j=0;j<nums.length;j++){
                if(j!=i && nums[i]>nums[j]){
                    count++;
                }
            ans[i]=count;
            }

        
            
        }
        return ans;
    }

}
//time complexity=0(n^2)
//Space complexity=0(n)



