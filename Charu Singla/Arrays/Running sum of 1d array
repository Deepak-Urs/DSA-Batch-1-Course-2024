class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        int sum=0;
        vector<int>total;
        for(int i=0;i<nums.size();i++){
            sum=sum+nums[i];
           total.push_back(sum);
        }
        return total;
    }
};

Time complexity : O(n)
Space complexity : O(n)
